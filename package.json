{
    "name": "figma-svelte-vite-template-2",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite & esbuild src/code/main.ts --bundle --outfile=dist/code.js --watch",
        "build": "vite build && esbuild src/code/main.ts --bundle --outfile=dist/code.js --minify",
        "dev:server": "vite dev --mode server & esbuild src/code/main.ts --define:process.env.NODE_ENV='\"server\"' --bundle --outfile=dist/code.js --watch"
    },
    "bin": {
        "plugma": "lib/plugma.js"
    },
    "devDependencies": {
        "@figma/plugin-typings": "^1.81.0",
        "@sindresorhus/slugify": "^2.2.1",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "@tsconfig/svelte": "^5.0.2",
        "inquirer": "^9.2.12",
        "lodash.template": "^4.5.0",
        "prettier": "^3.1.0",
        "prettier-plugin-svelte": "^3.1.0",
        "svelte": "^4.2.3",
        "svelte-check": "^3.6.0",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2",
        "vite": "^5.0.0",
        "vite-plugin-singlefile": "^0.13.5",
        "vite-svg-loader": "^5.0.1"
    }
}
